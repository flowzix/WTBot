<Window x:Class="WTBot.ActivationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WTBot"
        xmlns:p="clr-namespace:WTBot.Properties"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="{x:Static p:Resources.actWinTitle}" Height="156.757" Width="340.541">
    <Grid Margin="0,0,3.4,2.2">
        <Label Content="{x:Static p:Resources.actSerialKeyLabel}" Margin="72,0,71,0" HorizontalContentAlignment="Center" Height="30" VerticalAlignment="Top"/>
        <TextBox x:Name="serial_key" Margin="10,30,9,0" Height="41" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
        <Button x:Name="b_verify" Content="{x:Static p:Resources.actVerifyBut}" Margin="44,0,37,5" Height="33" VerticalAlignment="Bottom" Click="b_verify_Click"/>
    </Grid>
</Window>
